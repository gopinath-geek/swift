func swapTwoNumber(_ first: inout Int, _ second: inout Int){
    print("Numbers before swapping First is \(first) and Second is \(second)")
    first = first + second
    second = first - second
    first = first - second
    
    print("Numbers After swapping First is \(first) and Second is \(second)")
}

var first = 25
var second = 30
print("\n")
swapTwoNumber(&first, &second)
